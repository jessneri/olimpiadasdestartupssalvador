<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.olimpiadacubos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconeolimpiadas"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconeolimpiadas"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TenisActivity" android:screenOrientation="landscape"></activity>
        <activity android:name=".VoleiActivity" android:screenOrientation="landscape"/>
        <activity android:name=".BasqueteActivity" android:screenOrientation="landscape"/>
        <activity android:name=".FutebolActivity" android:screenOrientation="landscape" />
        <activity android:name=".EsportesActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>